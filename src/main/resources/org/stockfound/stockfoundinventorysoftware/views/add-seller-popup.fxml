<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="295.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.stockfound.stockfoundinventorysoftware.controllers.crud_controllers.crud_sellers.AddSellerViewController">
   <children>
      <Label alignment="CENTER" layoutX="184.0" layoutY="55.0" text="Name">
         <font>
            <Font name="Cambria Math" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="nameTextField" layoutX="310.0" layoutY="56.0" />
      <Button fx:id="addSellerButton" layoutX="352.0" layoutY="241.0" mnemonicParsing="false" onAction="#onClickAddSellerButton" text="Add Seller" />
      <Button fx:id="cancelButton" layoutX="188.0" layoutY="241.0" mnemonicParsing="false" onAction="#onClickCancelButton" text="Cancel" />
      <Label alignment="CENTER" layoutX="172.0" layoutY="102.0" text="Address">
         <font>
            <Font name="Cambria Math" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="addressTextField" layoutX="310.0" layoutY="104.0" />
      <Label alignment="CENTER" layoutX="137.0" layoutY="156.0" text="Phone Number">
         <font>
            <Font name="Cambria Math" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="phoneNumberTextField" layoutX="310.0" layoutY="158.0" />
   </children>
</AnchorPane>
